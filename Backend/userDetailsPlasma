const mongoose= require("mongoose")

const UserDetailsPlasmaSchema = new mongoose.Schema({
    fname : String,
    lname: String,
    bloodType: String,
    city: String,
    state: String,
    country: String,
    contactNo: String,
    ConpleteAddress: String, 
},
{
    Collection: "userInfoPlasma"
}
);

mongoose.model("userInfoPlasma", UserDetailsPlasmaSchema)
